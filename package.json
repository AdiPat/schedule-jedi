{
  "name": "schedule-jedi",
  "version": "0.0.1",
  "description": "Developer-first flexible scheduling library for Node.js. Schedule jobs with ease using a simple and intuitive API. Built with TypeScript and designed for modern JavaScript environments.",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "rm -rf dist/ && rm -rf types/ && tsc",
    "test": "vitest --testTimeout 30000",
    "test:silent": "vitest --silent --testTimeout 30000",
    "test:coverage": "vitest --coverage --testTimeout 30000",
    "test:coverage:silent": "vitest --coverage --silent --testTimeout 30000"
  },
  "files": [
    "dist",
    "types",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "scheduling",
    "scheduler",
    "job scheduler",
    "task scheduler",
    "nodejs",
    "typescript",
    "ai-sdk",
    "schedule-jedi"
  ],
  "author": "Aditya Patange (AdiPat) <contact.adityapatange@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.6",
    "@ai-sdk/openai": "^1.0.12",
    "ai": "^4.0.22",
    "axios": "^1.7.9",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "uuid": "^11.0.3",
    "vitest": "^3.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.1.1"
  }
}